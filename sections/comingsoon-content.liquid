{% if page_title == 'Alcohol | Eatoo UK' %}
  {% render 'age-check' %}
{% endif %}  

<div style="margin-bottom: 110px;" class="grid">
  <div class="grid__item large--five-sixths push--large--one-twelfth">

    <header class="section-header text-center">
      <div class="line-1 anim-typewriter"></div>
        <h2 style="font-size: 2em;">COMING SOON</h2> 
        <hr class="hr--small">
      
    </header>
  <div class="wrapper">
    <div class="grid">
      <div class="grid__item large--four-fifths push--large--one-tenth">
        <div class="rte rte--nomargin rte--indented-images">
          {% form 'customer' %}
            {{ form.errors | default_errors }}
            {% if form.posted_successfully? %}
              <div class="newsletter--form">
                <div class="note form-success">
                  {{ 'general.newsletter_form.newsletter_confirmation' | t }}
                </div>
                <br><br><br><br><br><br>
              </div>
            {% else %}
              <label for="Email" class="newsletter__label hidden-label">{{ 'general.newsletter_form.newsletter_email' | t }}</label>
              <input type="hidden" name="contact[tags]" value="newsletter">
              <div class="newsletter--form">
                <div class="rte text-center">
                  <div class="line-1 anim-typewriter">
                    <span>Stay tuned!</span>
                  </div>
                </div>
                <div class="anamatie-group">
                  <!--<div class="logo">
                    <img src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/food3.png?9208" />
                  </div>

                  <div class="wave-wrapper">
                    <div class="wave">
                      <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/312465/Moby%20Wave_White%20Fill.svg" />
                    </div>
                    <div class="cover-up"></div>
                  </div>-->
                  <i class="bounce">
                    <img style="height: 100px;" src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/moli2.png?9369" alt="Mooli" title="Mooli" />
                  </i>
                  <i class="bounce1">
                    <img style="height: 100px;"  src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/cabbage.png?9366" alt="Cabbage" title="Cabbage" />
                  </i>
                  <i class="bounce2">
                    <img style="height: 90px;"  src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/shroom.png?93676" alt="Mushroom" title="Mushroom" />
                  </i>
                  <i class="bounce3">
                    <img style="height: 100px;"  src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/veg.png?9366" alt="Vegetable" title="Vegetable" />
                  </i>
                  <i class="bounce4">
                    <img style="height: 100px;"  src="https://cdn.shopify.com/s/files/1/0093/0306/4631/files/meat.png?9366" alt="Meat" title="Meat" />
                  </i>
                </div>
                <div class="input-group">
                  <input type="email" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="{{ 'general.newsletter_form.newsletter_placeholder' | t }}" name="contact[email]" id="Email" class="input-group-field newsletter__input" autocorrect="off" autocapitalize="off" style="border-color: black; background-color: #222323; border-left-width: 1px; border-top-width: 1px; border-radius: 6px 0px 0px 6px;">
                  <span class="input-group-btn">
                    <button id="Subscribe" type="submit"
                      class="btn newsletter__submit"
                      aria-label="{{ 'general.newsletter_form.submit' | t }}" style="width: max-content;">
                      <span class="newsletter__submit-text--large">
                        {{ 'general.newsletter_form.submit' | t }}
                      </span>
                      <span class="newsletter__submit-text--small">
                        <span class="icon icon-arrow-right" aria-hidden="true"></span>
                      </span>
                    </button>
                  </span>
                </div>
              </div>
            {% endif %}
          {% endform %}
        </div>
      </div>
    </div>
  </div>
  </div>
</div>


<style type="text/css">
  .section-header.text-center {
    margin-bottom: unset;
  }
  
  .rte>div.line-1 {
    margin-bottom: 0px;
  }

  .line-1 {
    position: relative;
    top: 50%;  
    width: 5.7em;
    margin: 0 auto;
    border-right: 2px solid transparent;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
  }

  .anim-typewriter {
    animation: typewriter 4s steps(44) 1s 1 normal both,
              blinkTextCursor 500ms steps(44) infinite normal;
  }
  
  @keyframes typewriter {
    from {
      width: 0;
    }
    to {
      width: 5.7em;
    }
  }
  @keyframes blinkTextCursor {
    from,
    to {
      border-right-color: transparent;
    }
    50% {
      border-right-color: black;
    }
  }

  .anamatie-group {
    margin: auto;
    display: table;
    padding: 10px;
  }

  .bounce {
    display: inline-block;
    position: relative;
    -moz-animation: bounce 2s infinite linear;
    -o-animation: bounce 2s infinite linear;
    -webkit-animation: bounce 2s infinite linear;
    animation: bounce 2s infinite linear;
    colr:000;
  }

  .bounce1 {
    display: inline-block;
    position: relative;
    -moz-animation: bounce 2s infinite linear;
    -o-animation: bounce 2s infinite linear;
    -webkit-animation: bounce 2s infinite linear;
    animation: bounce 2s infinite linear;
    -webkit-animation-delay: .2s;
    animation-delay: .2s;
    colr:000;
  }

  .bounce2 {
    display: inline-block;
    position: relative;
    -moz-animation: bounce 2s infinite linear;
    -o-animation: bounce 2s infinite linear;
    -webkit-animation: bounce 2s infinite linear;
    animation: bounce 2s infinite linear;
    -webkit-animation-delay: .3s;
    animation-delay: .3s;
    colr:000;
  }

  .bounce3 {
    display: inline-block;
    position: relative;
    -moz-animation: bounce 2s infinite linear;
    -o-animation: bounce 2s infinite linear;
    -webkit-animation: bounce 2s infinite linear;
    animation: bounce 2s infinite linear;
    -webkit-animation-delay: .4s;
    animation-delay: .4s;
    colr:000;
  }

  .bounce4 {
    display: inline-block;
    position: relative;
    -moz-animation: bounce 2s infinite linear;
    -o-animation: bounce 2s infinite linear;
    -webkit-animation: bounce 2s infinite linear;
    animation: bounce 2s infinite linear;
    -webkit-animation-delay: .5s;
    animation-delay: .5s;
    colr:000;
  }

  @-webkit-keyframes bounce {
      0% { top: 0; }
      50% { top: -0.2em; }
      70% { top: -0.3em; }
      100% { top: 0; }
  }
  
  @-moz-keyframes bounce {
      0% { top: 0; }
      50% { top: -0.2em; }
      70% { top: -0.3em; }
      100% { top: 0; }
  }
  
  @-o-keyframes bounce {
      0% { top: 0; }
      50% { top: -0.2em; }
      70% { top: -0.3em; }
      100% { top: 0; }
  }
  
  @-ms-keyframes bounce {
      0% { top: 0; }
      50% { top: -0.2em; }
      70% { top: -0.3em; }
      100% { top: 0; }
  }
  
  @keyframes bounce {
      0% { top: 0; }
      50% { top: -0.2em; }
      70% { top: -0.3em; }
      100% { top: 0; }
  }

</style>

